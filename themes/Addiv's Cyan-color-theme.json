	"tokenColors": [
		{
			"name": "Comments",
			"scope": ["comment", "punctuation.definition.comment"],
			"settings": {
				"foreground": "#007c84",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Functions",
			"scope": ["entity.name.function", "meta.function-call"],
			"settings": {
				"foreground": "#00666b"
			}
		},
		{
			"name": "Keywords",
			"scope": ["keyword", "storage.modifier"],
			"settings": {
				"foreground": "#005b60",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Variables",
			"scope": ["variable", "variable.parameter"],
			"settings": {
				"foreground": "#0097a7"
			}
		},
		{
			"name": "Strings",
			"scope": ["string"],
			"settings": {
				"foreground": "#00acc1"
			}
		},
		{
			"name": "Numbers",
			"scope": ["constant.numeric"],
			"settings": {
				"foreground": "#00cde3"
			}
		},
		{
			"name": "Classes",
			"scope": ["entity.name.class", "entity.name.type.class"],
			"settings": {
				"foreground": "#005b60",
				"fontStyle": "bold"
			}
		}
	]
}
